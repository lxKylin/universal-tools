{
  "name": "universal-tools",
  "version": "0.0.5",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.js",
    "*.cjs"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx --max-warnings 0",
    "test": "vitest run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "utils",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:FungYuu/universal-tools"
  },
  "author": "FungYuu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FungYuu/universal-tools/issues"
  },
  "devDependencies": {
    "@ast-grep/napi-darwin-arm64": "^0.38.0",
    "@eslint/js": "^9.25.1",
    "@rslib/core": "^0.7.0",
    "@types/node": "^22.8.1",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vitepress": "2.0.0-alpha.5",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx,ts,tsx,json}": [
      "pnpm run lint",
      "pnpm run format"
    ]
  }
}
